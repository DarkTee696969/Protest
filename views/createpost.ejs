    <style>
        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            user-select: none;
        }

        @media (min-width: 768px) {
            .bd-placeholder-img-lg {
                font-size: 3.5rem;
            }
        }

    </style>
    <link href="/css/createpost.css" rel="stylesheet">


</head>
<body>
    
    <div class="square-box">
        <h3>Create Post</h3>
        <form action="/create-post" method="POST">
            <label for="content">Name</label>
            <input type="text" id="username" name="username" value="<%= typeof username !== 'undefined' ? username : '' %>">

            <label for="title">Title</label>
            <input type="text" id="title" name="title" value="<%= typeof title !== 'undefined' ? title : '' %>">
            
            <label for="content">Content</label>
            <textarea id="content" name="content" rows="5" cols="50"><%= typeof content !== 'undefined' ? content : '' %></textarea>
            
            <label for="position">Position</label>
            <input type="text" id="position" name="position" value="<%= typeof position !== 'undefined' ? position : '' %>">
            
            <p></p>
            <input type="submit" value="Add post">
            <div class="btn-container">
                <a class="btn-cancel" href="/myboard">Cancel</a>
            </div>
        </form>
    </div>
</body>
</html>
